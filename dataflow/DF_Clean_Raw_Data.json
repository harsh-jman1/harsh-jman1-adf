{
	"name": "DF_Clean_Raw_Data",
	"properties": {
		"folder": {
			"name": "Misumi"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL",
						"type": "DatasetReference"
					},
					"name": "TableInput"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_SQL",
						"type": "DatasetReference"
					},
					"name": "TableSink"
				}
			],
			"transformations": [
				{
					"name": "HashColumns"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> TableInput",
				"TableInput aggregate(groupBy(columns = sha2(256, columns())),",
				"     column1 = ERROR_FUNCTION('')) ~> HashColumns",
				"HashColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> TableSink"
			]
		}
	}
}
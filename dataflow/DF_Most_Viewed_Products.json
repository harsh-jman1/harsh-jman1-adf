{
	"name": "DF_Most_Viewed_Products",
	"properties": {
		"folder": {
			"name": "Misumi"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL",
						"type": "DatasetReference"
					},
					"name": "InputTable"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SortByViews"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> InputTable",
				"InputTable sort(asc(pageView, false),",
				"     caseInsensitive: true) ~> SortByViews"
			]
		}
	}
}